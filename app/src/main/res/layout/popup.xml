<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/line_popup"
		android:layout_width="250dp"
		android:layout_height="wrap_content"
		android:layout_margin="10dp"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/holder_top"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/line_forward"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/image_forward"
					android:focusable="false"
					android:layout_width="28dp"
					android:layout_height="28dp"
					android:src="@drawable/ic_chevron_right_black"
					android:scaleType="fitXY"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/line_reload"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/image_reload"
					android:focusable="false"
					android:layout_width="28dp"
					android:layout_height="28dp"
					android:src="@drawable/ic_loop_black"
					android:scaleType="fitXY"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/line_Download"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/image_download"
					android:focusable="false"
					android:layout_width="28dp"
					android:layout_height="28dp"
					android:src="@drawable/ic_get_app_black"
					android:scaleType="fitXY"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/line_find"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_find"
				android:focusable="false"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:src="@drawable/ic_search_black"
				android:scaleType="fitXY"/>
			<TextView
				android:id="@+id/text2"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="4dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Find in page"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/line_div2"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/line_pdf"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_savepdf"
				android:focusable="false"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:src="@drawable/ic_insert_drive_file_black"
				android:scaleType="fitXY"/>
			<TextView
				android:id="@+id/text3"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="4dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Save as PDF"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/line_div3"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/line_savepicture"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_savepicture"
				android:focusable="false"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:src="@drawable/ic_photo_black"
				android:scaleType="fitXY"/>
			<TextView
				android:id="@+id/text5"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="4dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Save as Picture"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/line_div1"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/line_editsaved"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_editsaved"
				android:focusable="false"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:src="@drawable/ic_create_black"
				android:scaleType="fitXY"/>
			<TextView
				android:id="@+id/text1"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="4dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Edit saved picture"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/line_div4"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/line_html"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_html"
				android:focusable="false"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:src="@drawable/ic_fast_forward_black"
				android:scaleType="fitXY"/>
			<TextView
				android:id="@+id/text6"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="4dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Load HTML"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/line_div5"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/line_settings"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/image_settings"
				android:focusable="false"
				android:layout_width="28dp"
				android:layout_height="28dp"
				android:src="@drawable/settings"
				android:scaleType="fitXY"/>
			<TextView
				android:id="@+id/text4"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="4dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Settings"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#000000"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
